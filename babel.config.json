{
  "presets": [
    [
      "@babel/preset-env",
      {
        "bugfixes": true,
        "targets": {
          "esmodules": true
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "babelrcRoots": [".", "packages/*"],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "@behave-graph/core": "@oveddan-behave-graph/core"
      }
    }]
  ]
}
