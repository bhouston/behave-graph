{
  "name": "@behave-graph/monorepo",
  "version": "0.9.10",
  "description": "Simple, extensible behavior graph engine",
  "private": true,
  "keywords": [
    "behavior",
    "graph",
    "flow"
  ],
  "homepage": "http://github.com/bhouston/behave-graph",
  "author": "behave-graph authors",
  "bugs": {
    "url": "https://github.com/bhouston/behave-graph/issues"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bhouston/behave-graph"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.43.0",
    "@web/dev-server": "^0.1.35",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.15",
    "typescript": "4.8.4"
  },
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --build --watch",
    "start": "npm run watch & npm run start --workspace=examples/graph-editor",
    "lint": "npx eslint \"{examples,packages}/**/src/**/*.{ts,tsx,json}\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier \"{examples,packages}/**/src/**/*.{ts,tsx,json}\" --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "docs": "typedoc",
    "test": "npm run test-flow && npm run test-core",
    "changeset:add": "changeset add",
    "vers": "npm run changeset version",
    "release": "npm run build && npm run changeset publish"
  },
  "workspaces": [
    "packages/core",
    "packages/flow",
    "examples/exec-graph",
    "examples/export-node-spec",
    "examples/graph-editor",
    "examples/three-viewer"
  ],
  "dependencies": {
    "@changesets/changelog-git": "^0.1.14",
    "@changesets/cli": "^2.26.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^3.2.4"
  }
}
